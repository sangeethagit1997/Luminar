package controlstatements;

public class Task2 {

	public static void main(String[] args) {
		// check which number is greater among the 3
		int p=20,q=400,r=90;
		if(p>q && p>r) {
		System.out.println("p is greater");
		}
		else if(q>p && q>r) {
			System.out.println("q is greater");
		}
		else {
			System.out.println("r is greater");
		}

	}

}
