package controlstatements;

public class ControlStatements {

	public static void main(String[] args) {
		// if
		int a=5;
		if(a>0) 
		{
			System.out.println("a is greater than zero");
		}
		
		//if else
		int c=10,d=15;
		if(c>10) {
			System.out.println("c is greater");
		}
		else
		{
			System.out.println("d is greater");
		}
		

	}

}
